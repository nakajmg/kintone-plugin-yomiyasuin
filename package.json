{
  "name": "kintone-plugin-svelte-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm-run-all -p compile:watch develop \"upload -- --watch\"",
    "build": "npm-run-all compile pack",
    "develop": "node develop.js",
    "upload": "export $(cat .env | xargs) && kintone-plugin-uploader dist/plugin.zip --waiting-dialog-ms 3000",
    "compile": "rollup -c",
    "compile:watch": "rollup -c -w",
    "pack": "kintone-plugin-packer --ppk private.ppk --out dist/plugin.zip .",
    "lint": "eslint src",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "dependencies": {
    "sirv-cli": "^1.0.0"
  },
  "devDependencies": {
    "@kintone/dts-gen": "^5.0.5",
    "@kintone/plugin-packer": "^5.0.16",
    "@kintone/plugin-uploader": "^6.0.1",
    "@kintone/rest-api-client": "^2.0.21",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@tsconfig/svelte": "^2.0.0",
    "chokidar": "^3.5.2",
    "dotenv": "^10.0.0",
    "lodash.debounce": "^4.0.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  }
}
